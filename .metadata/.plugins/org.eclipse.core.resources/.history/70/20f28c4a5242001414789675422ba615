package tmachine;

import java.io.FileNotFoundException;

public class Test {
	
	public static void main(String[]args){
		T16Machine tmach  = new T16Machine();
		TMachineProgram prog;
		
		try{
			prog = new TMachineProgram("prog1.tbc");
			tmach.load(prog);
			tmach.run();
		}
		catch(MalformedFileException e){
			e.printStackTrace();
		}
		catch(FileNotFoundException e){
			e.printStackTrace();
		}
		catch(IllegalOpcodeException e){
			e.printStackTrace();
		}
		
	}

}
